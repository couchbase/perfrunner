[test_case]
test = perfrunner.tests.analytics.BigFunQueryNoIndexWithCompressionTest
use_workers = 0

[showfast]
title = 4 nodes, BigFUN 20M users (320M docs), SSD, no indexes (ad hoc), Snappy
component = analytics
category = latency
sub_category = Without Index
orderby = _ssd_4n

[stats]
server_processes = java

[cluster]
mem_quota = 20480
analytics_mem_quota = 20480
initial_nodes = 6
num_buckets = 1

[analytics]
num_io_devices = 4
storage_compression_block = snappy

[bucket]
eviction_policy = fullEviction
compression_mode = active

[restore]
backup_storage = /backups
backup_repo = bigfun20M
threads = 8

[access]
analytics_warmup_ops = 3
analytics_warmup_workers = 1
ops = 10
workers = 1

[clients]
libcouchbase = 2.9.3
python_client = 2.5.0