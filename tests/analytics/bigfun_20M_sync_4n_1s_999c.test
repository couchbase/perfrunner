[test_case]
test = perfrunner.tests.analytics.BigFunSyncTest
use_workers = 0

[showfast]
title = Avg. ingestion rate (items/sec), 4 nodes, BigFUN 20M users (320M docs), 999 indexes, SSD, s=1 c=999
component = analytics
category = sync
sub_category = Initial
orderby = _ssd_4n_999

[stats]
server_processes = java

[cluster]
mem_quota = 20480
analytics_mem_quota = 51200
initial_nodes = 6
num_buckets = 1

[collection]
config = collections/1bucket_1scope_999collections_analytics.json

[analytics]
num_io_devices = 4
analytics_config_file = tests/analytics/config/999_datasets_collection_config.json

[bucket]
eviction_policy = fullEviction

[restore]
backup_storage = /data3/backup/999collections
backup_repo = default
threads = 8
map_data = bucket-1.scope-1=bucket-1.scope-1
