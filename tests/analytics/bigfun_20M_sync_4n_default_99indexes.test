[test_case]
test = perfrunner.tests.analytics.BigFunSyncTest
use_workers = 0

[showfast]
title = 4 nodes, BigFUN 20M users (320M docs), 99 indexes, SSD
component = analytics
category = sync
sub_category = Initial
orderby = _ssd_4n_99

[stats]
server_processes = java

[cluster]
mem_quota = 20480
analytics_mem_quota = 20480
initial_nodes = 6
num_buckets = 1

[analytics]
num_io_devices = 4
analytics_config_file = tests/analytics/config/99_datasets_default_config.json

[bucket]
eviction_policy = fullEviction

[restore]
backup_storage = /data3/backup/default99dataset
backup_repo = default
threads = 8
