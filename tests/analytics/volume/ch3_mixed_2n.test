[test_case]
test = perfrunner.tests.analytics.CH3Test

[showfast]
title = CH3 Queries, 2Q x 2A x 1F, c=11, mixed workload
component = analytics
category = ch3

[stats]
server_processes = java

[cluster]
mem_quota = 25600
index_mem_quota = 12800
analytics_mem_quota = 51200
fts_index_mem_quota = 51200
initial_nodes = 5
num_buckets = 1
bucket_name = bench

[collection]
config = collections/1bucket_1scope_11collections_analytics_ch3.json

[analytics]
num_io_devices = 4

[bucket]
eviction_policy = fullEviction
replica_index = 1

[compaction]
db_percentage = 100

[bucket_extras]
num_writer_threads = 64
num_reader_threads = 32
max_num_shards = 64

[secondary]
indexer.settings.storage_mode = plasma

[restore]
backup_storage = /data3/backup/ch3_w1000/
backup_repo = ch3
threads = 8
map_data = bench.ch3.customer=bench.ch3.customer,bench.ch3.district=bench.ch3.district,bench.ch3.history=bench.ch3.history,bench.ch3.item=bench.ch3.item,bench.ch3.neworder=bench.ch3.neworder,bench.ch3.orders=bench.ch3.orders,bench.ch3.stock=bench.ch3.stock,bench.ch3.warehouse=bench.ch3.warehouse,bench.ch3.supplier=bench.ch3.supplier,bench.ch3.nation=bench.ch3.nation,bench.ch3.region=bench.ch3.region,bench._default._default=bench._default._default

[ch3]
warehouses = 1000
aclients = 1
tclients = 64
fclients = 64
iterations = 2
warmup_iterations = 1
duration = 0
warmup_duration = 0
workload = ch3_mixed

[clients]
python_client = 4.0.2