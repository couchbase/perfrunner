[test_case]
test = perfrunner.tests.ai_services.AutoVecWorkflowTest

[showfast]
title = Autovec 20k docs x 1kb, Capella hosted model
component = aicloud
category = lat
sub_category = {provider}

[stats]
server_processes = eventing-produc

[cluster]
initial_nodes = 5
num_buckets = 1
enable_n2n_encryption = all

[bucket]
eviction_policy = fullEviction
backend_storage = magma

[load]
items = 20000
size = 1024
workers = 100
ssl_mode = capella

[ai_services]
workflow_type = structured
schema_fields = body
