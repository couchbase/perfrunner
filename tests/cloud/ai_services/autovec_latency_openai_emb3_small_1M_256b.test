[test_case]
test = perfrunner.tests.ai_services.AutoVecWorkflowTest

[showfast]
title = Autovec 1M docs (256B), OpenAI model
component = aiservices
category = autovec
sub_category = {provider}

[stats]
server_processes = eventing-produc

[cluster]
initial_nodes = 5
num_buckets = 1
enable_n2n_encryption = all

[bucket]
eviction_policy = fullEviction
backend_storage = magma

[load]
items = 1000000
size = 256
workers = 100
ssl_mode = capella

[access]
time = 0

[ai_services]
model_source = external
workflow_type = structured
model_name = text-embedding-3-small
schema_fields = body

[jts]
index_partitions = 1
vector_dimension = 1536
vector_similarity_type = dot_product
vector_index_optimized_for = recall
couchbase_index_configfile = tests/fts/index/fts_vector_l2_norm_only_vector_index.json
test_total_docs = 1000000
