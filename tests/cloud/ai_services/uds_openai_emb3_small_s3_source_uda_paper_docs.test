[test_case]
test = perfrunner.tests.ai_services.WorkflowIngestionAndLatencyTest
use_workers = 0

[showfast]
title = S3 (UDA/paper - 1128 PDFs), OpenAI (text-embedding-3-small)
component = aiservices
category = uds
sub_category = {provider}

[stats]
server_processes = eventing-produc

[cluster]
initial_nodes = 7
num_buckets = 1
enable_n2n_encryption = all
skip_metadata_bucket_creation = true
eventing_metadata_bucket_mem_quota = 1536

[bucket]
eviction_policy = fullEviction
backend_storage = magma

[access]
time = 0
ssl_mode = capella

[ai_services]
workflow_type = unstructured
model_source = external
model_name = text-embedding-3-small
s3_bucket = ai-uda-qa-pdfs
s3_path = paper_docs
chunking_strategy = RECURSIVE_SPLITTER
chunk_size = 2000

[jts]
index_partitions = 1
vector_dimension = 1536
vector_similarity_type = dot_product
vector_index_optimized_for = recall
couchbase_index_configfile = tests/fts/index/fts_vector_l2_norm_only_vector_index.json
test_total_docs = 0
