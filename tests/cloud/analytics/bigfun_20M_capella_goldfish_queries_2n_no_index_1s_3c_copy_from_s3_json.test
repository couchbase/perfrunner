[test_case]
test = perfrunner.tests.analytics.ColumnarCopyFromS3Test

[showfast]
title = 2 nodes, BigFUN 20M users (320M docs), no indexes (ad hoc), s=1 c=3, Capella Goldfish, COPY FROM S3, json
component = analyticscloud
category = latency
sub_category = {provider}

[stats]
server_processes = cbas java

[cluster]
initial_nodes = 2
enable_n2n_encryption = all

[backup]
obj_staging_dir = /stage
obj_region = us-east-1
aws_credential_path = /root/.ssh

[analytics]
queries = perfrunner/workloads/bigfun/queries_without_index_external.json
analytics_config_file = tests/analytics/config/3_datasets_collection_config.json
external_dataset_type = s3
external_dataset_region = us-east-1
external_bucket = analytics-bigfun20m-json
external_file_format = json
external_file_include = json
aws_credential_path = /root/.ssh/

[access]
ops = 5
workers = 1

[clients]
python_client = 4.0.2
