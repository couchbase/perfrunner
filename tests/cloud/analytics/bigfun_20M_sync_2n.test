[test_case]
test = perfrunner.tests.analytics.BigFunSyncAWSTest

[showfast]
title = Avg. ingestion rate (items/sec), 2 nodes, BigFUN 20M users (320M docs), 3 indexes, AWS/Capella
component = cloud
category = analytics
sub_category = AWS

[stats]
server_processes = java

[cluster]
mem_quota = 20480
analytics_mem_quota = 20480
initial_nodes = 5
num_buckets = 1
enable_cpu_cores = false
ui_http = disabled
enable_n2n_encryption = all

[analytics]
num_io_devices = 8

[bucket]
eviction_policy = fullEviction
failover_min = 10

[backup]
obj_staging_dir = /root
obj_region = us-east-1
aws_credential_path = /root/.ssh

[restore]
backup_storage = s3://analytics-bigfun20m
backup_repo = bigfun20M
threads = 8

[clients]
libcouchbase = 2.9.3
python_client = 2.5.0