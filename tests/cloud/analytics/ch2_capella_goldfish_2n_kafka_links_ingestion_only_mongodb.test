[test_case]
test = perfrunner.tests.analytics.CH2ColumnarKafkaLinksIngestionTest

[showfast]
title = Kafka Links Ingestion rate (avg. items/sec), CH2 1000 warehouses, c=11, 2 Analytics nodes, Capella Goldfish, AWS
component = analyticscloud
category = ch2
sub_category = {provider}

[stats]
server_processes = java

[cluster]
initial_nodes = 2
enable_n2n_encryption = all

[analytics]
analytics_link = KafkaLink

[goldfish_kafka_links]
link_source = MONGODB
remote_database_name = ch2
primary_key_field = _id

[backup]
obj_staging_dir = /stage
obj_region = us-east-1
aws_credential_path = /root/.ssh

[clients]
python_client = 4.0.2
