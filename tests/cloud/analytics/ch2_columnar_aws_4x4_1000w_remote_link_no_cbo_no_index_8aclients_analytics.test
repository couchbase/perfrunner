[test_case]
test = perfrunner.tests.analytics.CH2RemoteLinkTest

[showfast]
title = CH2 1000W, 4A, c=11 (remote collections), analytics-only workload, No CBO, Hand-Optimized Queries, 8 aclients
component = analyticscloud
category = ch2
sub_category = {provider}

[stats]
server_processes = java

[cluster]
mem_quota = 256
analytics_mem_quota = 25600
initial_nodes = 4 4
num_buckets = 1
bucket_name = bench

[backup]
obj_staging_dir = /stage
obj_region = us-east-1
aws_credential_path = /root/.ssh

[restore]
backup_storage = gs://analytics-ch2-1000w
backup_repo = ch2
threads = 12
map_data = bench.ch2=bench.ch2

[analytics]
analytics_link = RemoteLink
coresMultiplier = 8

[ch2]
warehouses = 1000
aclients = 8
tclients = 0
load_tclients = 48
iterations = 2
warmup_iterations = 1
duration = 0
warmup_duration = 0
workload = ch2_analytics
use_backup = true

