[test_case]
test = perfrunner.tests.analytics.CH2Test

[showfast]
title = CH2++64 4000W, 4A, c=11 (remote collections), mixed workload, 64 tclients, No CBO, Hand-optimized Queries, Analytics Indexes
component = analyticscloud
category = ch2
sub_category = {provider}

[stats]
server_processes = java

[cluster]
mem_quota = 100
analytics_mem_quota = 25600
initial_nodes = 4 4
num_buckets = 1
bucket_name = bench

[bucket]
eviction_policy = fullEviction

[collection]
config = collections/1bucket_1scope_11collections_analytics_ch2pp.json

[analytics]
num_io_devices = 1
index_conf_file = tests/analytics/config/ch2_analytics_secondary_indexes.json
analytics_link = RemoteLink

[backup]
obj_staging_dir = /stage
obj_region = us-east-1
aws_credential_path = /root/.ssh

[restore]
backup_storage = s3://analytics-ch2pp64-4000w
backup_repo = ch2
threads = 8
map_data = bench.ch2pp=bench.ch2pp

[ch2]
schema = ch2pp
warehouses = 4000
aclients = 1
tclients = 64
load_tclients = 48
iterations = 2
warmup_iterations = 1
duration = 0
warmup_duration = 0
workload = ch2_mixed
create_gsi_index = true
customer_extra_fields = 64
orders_extra_fields = 64
item_extra_fields = 64
datagen_seed = 12345