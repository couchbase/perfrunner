[test_case]
test = perfrunner.tests.analytics.ColumnarCopyToObjectStoreTest

[showfast]
title = Copy To S3, CH2++0 1000W, 4A, c=11 (standalone collections)
component = analyticscloud
category = latency
sub_category = {provider}

[stats]
server_processes = java

[cluster]
mem_quota = 100
analytics_mem_quota = 25600
initial_nodes = 4

[analytics]
external_dataset_type = s3
external_dataset_region = us-east-1
external_bucket = analytics-ch2pp0-1000w-gz
external_file_format = json
external_file_include = gz

[ch2]
schema = ch2pp

[columnar]
object_store_import_datasets = customer_no_empty_arrays:customer, stock, orders

[columnar_copy_to]
object_store_file_formats = json, parquet
max_objects_per_files = 10000
object_store_compressions = gzip, zstd
object_store_query_file = tests/cloud/analytics/workload/ch2pp_copy_to_blob_store_parquet_queries.yaml
pairwise_param_combinations = true
