[test_case]
test = perfrunner.tests.vectordb.VectorDBBenchTest

[showfast]
title = 4 nodes, 1 bucket, FTS
component = jtscloud
category = vectordb
sub_category = {provider}

[cluster]
mem_quota = 51200
initial_nodes = 8
num_buckets = 1
fts_index_mem_quota = 25000
enable_cpu_cores = false
enable_n2n_encryption = all

[vectordb]
cases = Performance1536D5M
ssl_mode = capella
dataset_path = /stage

[profiling]
services = fts