[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = Avg Throughput (ops/sec), Workload A, 3 nodes, 1 bucket x 1500M x 1KB
component = kvcloud
category = max_ops
sub_category = {provider}
orderby = 1500m

[cluster]
initial_nodes = 3
num_buckets = 1
enable_cpu_cores = false
enable_n2n_encryption = all

[bucket]
eviction_policy = fullEviction
backend_storage = magma

[load]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 1500000000
workers = 100
ssl_mode = capella

[access]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 1500000000
ops = 1000000000
time = 1200
boost = 12
workers = 400
workload_instances = 2
kv_endpoints = 16
ssl_mode = capella

[ycsb]
branch = couchbase3

[stats]
client_processes = java
