[test_case]
test = perfrunner.tests.ycsb.YCSBLatencyTest

[showfast]
title = Latency(ms), Workload A, 3 nodes, 1 bucket x 375M x 1KB, 15K ops/sec, 128 vBuckets
component = kvcloud
category = latency
sub_category = {provider}
orderby = 375m

[cluster]
initial_nodes = 3
num_buckets = 1
num_vbuckets = 128
enable_cpu_cores = false
enable_n2n_encryption = all

[bucket]
eviction_policy = fullEviction
backend_storage = magma

[load]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 375000000
workers = 100
ssl_mode = capella

[access]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 375000000
target = 4000
ops = 0
time = 1200
boost = 12
workers = 50
timeseries = 1
workload_instances = 2
kv_endpoints = 16
ssl_mode = capella

[ycsb]
latency_percentiles = 50,99
average_latency = 1
branch = couchbase3

[stats]
client_processes = java
