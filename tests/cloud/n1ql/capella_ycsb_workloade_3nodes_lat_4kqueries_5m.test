[test_case]
test = perfrunner.tests.ycsb.YCSBN1QLLatencyTest

[showfast]
title = Latency (ms), Workload E, 5 nodes, 1 bucket x 5M x 1KB, 4K queries/sec
component = n1qlcloud
category = ycsb
sub_category = {provider}
orderby = 5m_l

[cluster]
mem_quota = 12800
index_mem_quota = 11264
initial_nodes = 5
num_buckets = 1
enable_cpu_cores = false
enable_n2n_encryption = all

[bucket]
replica_number = 1
eviction_policy = fullEviction
backend_storage = magma

[secondary]
indexer.settings.storage_mode = plasma

[index]
statements =
    CREATE PRIMARY INDEX primary_idx ON `bucket-1` WITH {"num_replica": 1};

[load]
workload_path = workloads/workloade
ycsb_client = couchbase3
items = 5000000
workers = 100
ssl_mode = capella

[access]
workload_path = workloads/workloade
ycsb_client = couchbase3
items = 5000000
target = 4000
ops = 0
time = 1200
workers = 400
workload_instances = 1
timeseries = 1
kv_endpoints = 16
ssl_mode = capella

[ycsb]
branch = couchbase3
latency_percentiles = 99
average_latency = 1

[stats]
client_processes = java
