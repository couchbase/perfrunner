[test_case]
test = perfrunner.tests.ycsb.YCSBN1QLThroughputTest

[showfast]
title = Avg Throughput (queries/sec), Workload E, 5 nodes, AWS/Capella, m5.4xlarge
component = cloud
category = n1ql
orderby = c_large

[cluster]
mem_quota = 40960
index_mem_quota = 40960
initial_nodes = 5
num_buckets = 1
enable_cpu_cores = false
ui_http = disabled
enable_n2n_encryption = all

[bucket]
replica_number = 1
eviction_policy = fullEviction
failover_min = 10

[secondary]
indexer.settings.storage_mode = plasma

[load]
workload_path = workloads/workloade
ycsb_client = couchbase3
items = 20000000
workers = 160

[index]
statements =
    CREATE PRIMARY INDEX primary_idx ON `bucket-1`;

[access]
workload_path = workloads/workloade
ycsb_client = couchbase3
items = 20000000
ops = 1000000000
time = 1800
workers = 600
workload_instances = 6
kv_endpoints = 16
ssl_mode = data
show_tls_version = true
min_tls_version = tlsv1.2

[ycsb]
branch = couchbase3

[stats]
client_processes = java
server_processes = projector prometheus
