[test_case]
test = perfrunner.tests.tools.ImportTest

[showfast]
title = Import JSON Lines (Avg. MB/sec), 4 nodes, 1 bucket x 60M x 1KB, Idle, s=1, c=1
component = tools
category = import

[stats]
client_processes = cbimport

[cluster]
mem_quota = 52428
initial_nodes = 4
num_buckets = 1

[collection]
config = tests/collections/utils/configs/1bucket_1scope_1collection_basic.json

[compaction]
db_percentage = 100

[load]
items = 60000000
size = 1024
workers = 40
doc_gen = import_export_simple

[export]
threads = 16
format = lines
type = json
collection_field = collection
scope_field = scope
scope_collection_exp = scope-1.collection-1


[clients]
libcouchbase = 3.0.2
python_client = git+https://github.com/couchbase/couchbase-python-client@a193cbdc7763aa3fa563bd8d139091a4c220a240