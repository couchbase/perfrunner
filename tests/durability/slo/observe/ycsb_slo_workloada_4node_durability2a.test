[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = replicate_to=0 persist_to=0, 1 client, 128 threads, YCSB SLO, SDK 2, 4 nodes, Workload A
component = kv
category = durability
sub_category = SLO
orderby = a201

[cluster]
mem_quota = 62000
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 2

[load]
workload_path = workloads/workloada
items = 20000000
workers = 128
ycsb_client = couchbase2

[access]
workload_path = workloads/workloada
replicate_to = 0
persist_to = 0
items = 20000000
ops = 100000000
time = 120
workers = 128
workload_instances = 1
ycsb_client = couchbase2
