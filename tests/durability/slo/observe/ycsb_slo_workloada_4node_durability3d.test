[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = replicate_to=1 persist_to=2, YCSB SLO, SDK 3, replicate_to=1 persist_to=2, 4 nodes, Workload A
component = kv
category = durability
sub_category = SLO
orderby = d301

[cluster]
mem_quota = 62000
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 2

[load]
workload_path = workloads/workloada
items = 20000000
workers = 128
ycsb_client = couchbase3

[access]
replicate_to = 1
persist_to = 2
workload_path = workloads/workloada
items = 20000000
ops = 100000000
time = 1200
workers = 128
workload_instances = 1
ycsb_client = couchbase3
enable_mutation_token = true
kv_endpoints = 16
timeseries = 1

[ycsb]
branch = couchbase3-new-durability
latency_percentiles = 98
