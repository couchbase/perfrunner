[test_case]
test = perfrunner.tests.elastic.ElasticThroughputTest
title =  OrHighHigh, no kv-load, wiki 1M x 1KB
summary = ElasticSearch throughput Test
larger_is_better = true
fts = true

[access]
time = 600

[cluster]
mem_quota = 51024
initial_nodes = 1
num_buckets = 1
fts_index_mem_quota = 20000

[fts]
items = 1000000
name  = perf_elastic_index
port = 8094
worker= 20
type = 2_disjuncts
query_file = tests/fts/data/hi_hi.txt
query_size = 10
logfile = /tmp/fts_thput_orhihi_1M_1K.txt
orderby = q0b00000111
backup_path = /home/backup
repo_path = wiki1M
field = text
elastic = 1