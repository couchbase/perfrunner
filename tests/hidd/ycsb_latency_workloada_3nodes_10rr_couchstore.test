[test_case]
test = perfrunner.tests.magma.YCSBLatencyHiDDTest

[showfast]
title = Latency(ms), Workload A, 3 nodes, 1 bucket x 1.5B x 1KB, Uniform distribution, 15K ops/sec, 10%% Resident Ratio, Couchstore
component = hidd
category = ycsb
orderby = ac
sub_category = Latency

[cluster]
mem_quota = 102400
initial_nodes = 3
num_buckets = 1
kernel_mem_limit = 120G
kernel_mem_limit_services = kv
enable_cpu_cores = false

[bucket]
replica_number = 1
eviction_policy = fullEviction

[bucket_extras]
num_reader_threads = 64
num_writer_threads = 16

[load]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 1500000000
workers = 320
kv_endpoints = 16
size = 1000

[access]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 1500000000
ops = 0
target = 2500
time = 1800
boost = 12
workers = 320
kv_endpoints = 16
workload_instances = 6
timeseries = 1
requestdistribution = uniform

[ycsb]
latency_percentiles = 50,90,99
branch = couchbase3

[stats]
client_processes = java
