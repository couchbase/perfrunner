[test_case]
test = perfrunner.tests.magma.YCSBLatencyHiDDTest

[showfast]
title = Latency(ms), Workload C, 3 nodes, 5 buckets x 300M x 1KB, Uniform distribution, 15K ops/sec, 10%% Resident Ratio, Magma
component = hidd
category = ycsb
orderby = cm
sub_category = Latency

[cluster]
mem_quota = 102400
initial_nodes = 3
num_buckets = 5
kernel_mem_limit = 120G
kernel_mem_limit_services = kv
enable_cpu_cores = false

[bucket]
replica_number = 1
eviction_policy = fullEviction
backend_storage = magma

[bucket_extras]
num_reader_threads = 64
num_writer_threads = 16

[load]
workload_path = workloads/workloadc
ycsb_client = couchbase3
items = 300000000
workers = 640
kv_endpoints = 128
size = 1000
use_backup = true

[extra_access]
run_extra_access = true
workload_path = workloads/workloadupdate
ycsb_client = couchbase3
items = 300000000
ops = 300000000
time = 86400
boost = 12
workers = 640
kv_endpoints = 128
workload_instances = 1
requestdistribution = uniform

[access]
workload_path = workloads/workloadc
ycsb_client = couchbase3
items = 300000000
ops = 0
target = 3000
time = 1800
boost = 12
workers = 640
kv_endpoints = 64
workload_instances = 1
timeseries = 1
requestdistribution = uniform

[ycsb]
latency_percentiles = 50,90,99
branch = couchbase3

[stats]
client_processes = java

[backup]
backup_directory = /data2/3n_5b_1500m_1kb
