[test_case]
test = perfrunner.tests.kv.MemoryOverheadTest

[showfast]
title = Memory overhead (%%), 1 bucket x 20M x 256B, Ephemeral
component = kv
category = memory

[stats]
interval = 1

[cluster]
mem_quota = 51200
initial_nodes = 4
num_buckets = 1

[bucket]
bucket_type = ephemeral
eviction_policy = nruEviction

[load]
items = 20000000
size = 256
workers = 10

[access]
time = 300

[clients]
libcouchbase = 3.3.12