[test_case]
test = perfrunner.tests.n1ql.N1QLThroughputTest

[showfast]
title = Q1, Key-Value Lookup, 30 buckets, idle
component = n1ql
category = Q1_Q3
sub_category = KV

[cluster]
mem_quota = 20480
index_mem_quota = 100000
initial_nodes = 6
num_buckets = 100

[compaction]
