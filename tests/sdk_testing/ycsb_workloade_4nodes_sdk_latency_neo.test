[test_case]
test = perfrunner.tests.ycsb.YCSBN1QLLatencyTest

[showfast]
title = Latency (ms), Workload E, MOI, 4 nodes
component = sdk
category = java
sub_category = neo

[cluster]
mem_quota = 40960
index_mem_quota = 10240
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 0

[secondary]
indexer.settings.storage_mode = memory_optimized

[load]
workload_path = workloads/workloade
items = 20000000
workers = 40

[index]
statements =
    CREATE PRIMARY INDEX primary_idx ON `bucket-1` WITH {"num_replica": 3};

[access]
workload_path = workloads/workloade
items = 20000000
target = 1500
ops = 0
time = 1200
workers = 60
workload_instances = 6
timeseries = 1
kv_endpoints = 1024

[sdktesting]
enable_sdktest = 1
sdk_type = java

[ycsb]
branch = couchbase3
latency_percentiles = 99
average_latency = 1

[stats]
client_processes = java