[test_case]
test = perfrunner.tests.syncgateway.EndToEndMultiCBLPullTest

[showfast]
title = 1 node, 100 cbl, 1M docs, 10 fields, 10Kb docSize
component = syncgateway
category = e2e
sub_category = Pull

[cluster]
mem_quota = 25000
index_mem_quota = 25000
initial_nodes = 4
num_buckets = 1

[access]
time = 1200
workers = 0

[secondary]
indexer.settings.storage_mode = memory_optimized

[syncgateway]
nodes = 1
load_threads = 100
threads = 100
clients = 20
instances_per_client = 5
workload_path = workloads/syncgateway_blank
ycsb_repo = git://github.com/couchbaselabs/YCSB.git
ycsb_branch = tmp-sqw-weekly-updated
users = 100
channels = 100
channels_per_user = 1
channels_per_doc = 1
documents = 1000000
roundtrip_write = false
feed_reading_mode = idsonly
insert_mode = bykey
auth = false
grant_access = false
starchannel = false
readproportion=0
updateproportion=1
insertproportion=0
requestdistribution = sequential
replication_type = E2E_PULL
writeallfields = false
updatefieldcount = 1
fieldlength = 1000
ramdisk_size = 8000
cbl_throughput = 10000
