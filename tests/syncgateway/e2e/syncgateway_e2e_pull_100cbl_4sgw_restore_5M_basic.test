[test_case]
test = perfrunner.tests.syncgateway.EndToEndMultiCBLPullRestoreTest

[showfast]
title = 4 nodes, 100 cbl, 5M docs, openai
component = syncgateway
category = e2e
sub_category = Pull

[cluster]
mem_quota = 25000
index_mem_quota = 25000
initial_nodes = 4
num_buckets = 1

[load]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 5000000
workers = 160
workload_instances = 1

[access]
workload_path = workloads/workloada
ycsb_client = couchbase3
items = 5000000
ops = 90000000
workers = 160
time = 1200
workload_instances = 1
kv_endpoints = 16

[secondary]
indexer.settings.storage_mode = memory_optimized

[backup]
obj_staging_dir = /stage
obj_region = us-east-1
aws_credential_path = /root/.ssh

[restore]
backup_storage = s3://fts-vector-openai-5m-1536dim
backup_repo = fts_openai_large_5m_1536_dims
use_csp_specific_archive = True
threads = 20

[syncgateway]
nodes = 4
load_threads = 100
threads = 100
clients = 4
instances_per_client = 25
workload_path = workloads/syncgateway_blank
ycsb_repo = git://github.com/couchbaselabs/YCSB.git
ycsb_branch = tmp-sqw-weekly-updated-c3
ycsb_command = syncgateway3
users = 100
channels = 100
channels_per_user = 1
channels_per_doc = 1
documents = 5000000
roundtrip_write = false
feed_reading_mode = idsonly
insert_mode = bykey
auth = false
grant_access = false
starchannel = false
pullproportion = 1
pushproportion = 0
readproportion = 0
updateproportion = 1
insertproportion = 0
requestdistribution = sequential
replication_type = E2E_PULL
writeallfields = false
updatefieldcount = 1
fieldlength = 100
ramdisk_size = 1000
cbl_throughput = 10000
mem_cpu_stats = true