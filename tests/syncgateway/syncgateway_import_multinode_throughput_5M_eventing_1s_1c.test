worker_count = 24
cpp_worker_thread_count = 2
config_file = tests/eventing/config/function_sample_bkt_1s_1c.json

[load]
workload_path = workloads/workloadcl
ycsb_client = couchbase3
items = 5000000
workers = 160
workload_instances = 2

[access]
