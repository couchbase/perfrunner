[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest
title = Avg Throughput (ops/sec), Workload A, 3 nodes, 12 vCPU, Ephemeral, no replication
component = kv
category = ycsb

[cluster]
mem_quota = 40960
initial_nodes = 3
num_buckets = 1
online_cores = 6

[bucket]
bucket_type = ephemeral
eviction_policy = nruEviction
replica_number = 0

[load]
workload_path = workloads/workloada
items = 20000000
workers = 160

[access]
workload_path = workloads/workloada
items = 20000000
ops = 1000000000
time = 1800
workers = 120
worker_instances = 6
