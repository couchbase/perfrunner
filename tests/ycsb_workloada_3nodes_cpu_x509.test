[test_case]
test = perfrunner.tests.ycsb.YCSBThroughputTest

[showfast]
title = Avg Throughput (ops/sec), Workload A, 3 nodes, 12 vCPU, SSL, X.509 Auth
component = kv
category = ycsb

[cluster]
mem_quota = 40960
initial_nodes = 3
num_buckets = 1
online_cores = 6

[load]
workload_path = workloads/workloada
items = 20000000
workers = 160

[access]
workload_path = workloads/workloada
items = 20000000
ops = 1000000000
time = 1800
boost = 12
workers = 96
workload_instances = 6
ssl_mode = auth
ycsb_jvm_args = "-Djdk.tls.client.protocols=TLSv1.2"

[ycsb]
branch = tmp-x509
