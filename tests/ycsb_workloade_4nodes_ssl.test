[test_case]
test = perfrunner.tests.ycsb.YCSBN1QLThroughputTest

[showfast]
title = Avg Throughput (queries/sec), Workload E, MOI, SSL, 4 nodes
component = n1ql
category = ycsb
sub_category = MOI

[cluster]
mem_quota = 40960
index_mem_quota = 10240
initial_nodes = 4
num_buckets = 1

[compaction]
db_percentage = 100

[bucket]
replica_number = 0

[secondary]
indexer.settings.storage_mode = memory_optimized

[load]
workload_path = workloads/workloade
items = 20000000
workers = 40

[index]
statements =
    CREATE PRIMARY INDEX primary_idx ON `bucket-1` WITH {"num_replica": 3};

[access]
workload_path = workloads/workloade
items = 20000000
ops = 1000000000
time = 600
workers = 120
workload_instances = 10
ssl_mode = data

[ycsb]
branch = tmp-x509

[stats]
client_processes = java